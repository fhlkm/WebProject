<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.4">
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>logincookie.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>upload</servlet-name>
    <servlet-class>com.hmkcode.UploadServlet</servlet-class>
    <init-param>
      <description>Define storagepath for fileuploads</description>
      <param-name>upload_path</param-name>
      <param-value>/media/data/fileUpload</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>upload</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FirstServlet</servlet-name>
    <servlet-class>com.jspservletcookbook.FirstServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FirstServlet</servlet-name>
    <url-pattern>/firstservlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SecondServlet</servlet-name>
    <servlet-class>com.jspservletcookbook.SecondServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SecondServlet</servlet-name>
    <url-pattern>/secondservlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>login</servlet-name>
    <servlet-class>com.jspservletcookbook.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
<!--   selection page -->
    <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CheckAccount</servlet-name>
    <servlet-class>com.jspservletcookbook.CheckAccount</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CheckAccount</servlet-name>
    <url-pattern>/initalSelect</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CheckSelection</servlet-name>
    <servlet-class>com.jspservletcookbook.CheckSelection</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CheckSelection</servlet-name>
    <url-pattern>/selection</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SelectionShow</servlet-name>
    <servlet-class>com.jspservletcookbook.ShowSelections</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SelectionShow</servlet-name>
    <url-pattern>/selectionShow</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>sendParameters</servlet-name>
    <servlet-class>com.jspservletcookbook.Parameters</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sendParameters</servlet-name>
    <url-pattern>/Parameters</url-pattern>
  </servlet-mapping>
  
<!--   show the equipment -->

  <servlet>
    <servlet-name>sendParts</servlet-name>
    <servlet-class>com.jspservletcookbook.CheckDeviceParts</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sendParts</servlet-name>
    <url-pattern>/parts</url-pattern>
  </servlet-mapping>
  
<!--   Test -->
  <servlet>
    <servlet-name>test</servlet-name>
    <servlet-class>com.jspservletcookbook.test.ReadFileTest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
  
<!--   Receive message from client -->
  
    <servlet>
    <servlet-name>sendmsg</servlet-name>
    <servlet-class>com.jspservletcookbook.ReceiveMessage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sendmsg</servlet-name>
    <url-pattern>/sendmsg</url-pattern>
  </servlet-mapping>
  
</web-app>